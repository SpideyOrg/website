    <div id="initial_bar" style="width: 100%; height: 8px; background-color: var(--bb);"></div>
    
    <br>
    <br>
    <br>
    <br>
    <br>

    <div class="tableModel">
      <center>
          <div class="bob">
            <img src="<%- bot.avatar %>" alt="..." class="rounded-circle" style="height: 300px; pointer-events: none; -webkit-box-shadow: 9px 7px 46px 0px rgba(0,0,0,0.80); box-shadow: 9px 7px 46px 0px rgba(0,0,0,0.80);">
          </div>
        <br>
        <br>
        <br>
          <h2 class="card-title" style="color: white; font-size: 275%"><strong><%- bot.user.username %></strong></h2>
          <h2 class="card-title" style="color: white; padding: 8px; font-size: 235%"><strong>O Bot com os melhores recursos da Aoi.js para você!</strong></h2>
      </center>
    </div>

    <br>
    <br>

      <div class="tableModel">
        <center>
            <a href="discordapp.com/oauth2/authorize?client_id=763109929300262953&permissions=8&scope=identify bot" target="_black">
              <button type="button"class="btn" style="background-color: color: #3B32EF;">Me adicione!</button>
            </a>
            <a href="https://discord.com/invite/j4aQnBUqKz/" target="_black">
              <button type="button" class="btn" style="background-color: color: #3B32EF;">Servidor de Suporte</button>
            </a>
            <!-- <a href="" target="_black">
              <button type="button" class="btn" style="background-color: color: #3B32EF;">Vote em mim!</button>
            </a> -->
