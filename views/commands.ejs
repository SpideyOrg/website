      </div>

    <br>
    <br>
    <br>
    <br>
    
       <div class="cards">
  <table class="table table-dark">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Primeiro</th>
      <th scope="col">Último</th>
      <th scope="col">Nickname</th>
    </tr>
  </thead>
  <tbody>
    <% commands.map(cmd => { %>
      <tr>
        <th scope="row"><%- ++i %></th>
        <td><%- cmd.name %></td>
        <td>Otto</td>
        <td>@mdo</td>
      </tr>
    <% }) %>
    <tr>
  </tbody>
</table>
       </div>

      
  </body>
</html>